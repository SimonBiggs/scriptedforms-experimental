{
  "name": "@scriptedforms/core",
  "version": "0.1.0",
  "description": "The core package for ScriptedForms",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/SimonBiggs/scriptedforms-experimental",
  "bugs": {
    "url": "https://github.com/SimonBiggs/scriptedforms-experimental/issues"
  },
  "license": "AGPL-3.0+",
  "author": "SimonBiggs",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/SimonBiggs/scriptedforms-experimental.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",

    "watch": "tsc -w",
    "install:lab": "jupyter labextension install --no-build ."
  },
  "dependencies": {
    "@jupyterlab/application": "^0.19.1"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.1.1"
  },
  "jupyterlab": {
    "extension": true
  }
}
